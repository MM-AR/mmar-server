{
  "name": "mmar-server",
  "version": "1.1.0",
  "description": "This the MMAR server for the MMAR project",
  "scripts": {
    "start": "npm run tjs && npm run swagger && npm run build && node ../dist/mmar-server/index.js",
    "debug": "nodemon",
    "build": "npm run build:global && npm run build:server",
    "build:server": "npm install && copyfiles \"package*.json\" \"../dist/mmar-server\" && npm install --prefix ../dist/mmar-server && tsc --build && npm run postbuild:server",
    "postbuild:server": "copyfiles \"config/**/*\" \"../dist/mmar-server\" && copyfiles \"views/**/*\" \"../dist/mmar-server\"",
    "build:global": "cd ../mmar-global-data-structure && npm run build",
    "test": "mocha -r ts-node/register test/unitTests/**/*.spec.ts",
    "swagger": "node ./config/swagger.mjs",
    "tjs": "node ./config/tjs.mjs"
  },
  "nodemonConfig": {
    "watch": [
      "./"
    ],
    "ext": ".ts,.js",
    "ignore": [
      "./**/*.spec.ts"
    ],
    "exec": "ts-node ./index.ts"
  },
  "author": "Daniel Borcard & Fabian Muff & Gunakar Challa",
  "license": "AGPL-3.0",
  "dependencies": {
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.7",
    "copyfiles": "2.4.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "ejs": "3.1.10",
    "express": "4.21.2",
    "express-ejs-layouts": "2.5.1",
    "express-winston": "4.2.0",
    "helmet": "7.1.0",
    "json-rules-engine": "7.2.1",
    "jsonpath-plus": "10.2.0",
    "jsonwebtoken": "9.0.2",
    "multer": "1.4.5-lts.1",
    "node-rules": "github:omgimalexis/node-rules",
    "pg": "8.12.0",
    "reflect-metadata": "0.2.2",
    "routes": "2.1.0",
    "source-map-support": "0.5.21",
    "swagger-autogen": "2.23.7",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "5.0.1",
    "typescript-json-schema": "0.65.1",
    "uuidv4": "6.2.13",
    "winston": "3.13.1"
  },
  "devDependencies": {
    "@stekoe/ocl.js": "1.4.3",
    "@types/bcrypt": "5.0.2",
    "@types/chai": "4.3.17",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/ejs": "3.1.5",
    "@types/express": "4.17.21",
    "@types/express-ejs-layouts": "2.5.4",
    "@types/jsonwebtoken": "9.0.6",
    "@types/mocha": "10.0.10",
    "@types/multer": "1.4.11",
    "@types/pg": "8.11.6",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "chai": "4.3.10",
    "chai-http": "4.4.0",
    "mocha": "10.8.2",
    "nodemon": "3.1.4",
    "prettier": "3.4.2",
    "rools": "2.3.0",
    "ts-node": "10.9.2",
    "typedoc": "0.27.4",
    "typescript": "5.7.2"
  }
}